{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"strictNullChecks": true,
		"lib": ["ESNext"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"include": ["./src/**/*.ts", "./*.ts", "./env.d.ts"]
}
